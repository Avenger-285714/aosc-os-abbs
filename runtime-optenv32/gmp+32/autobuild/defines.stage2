PKGNAME=gmp+32
PKGSEC=libs
PKGDEP="glibc+32"
BUILDDEP="devel-optenv32-base"
PKGDES="GNU Multiprecision library (optenv32)"

# Disabling C++ support since stage 3 compiler is not yet available.
AUTOTOOLS_AFTER=(
	"--enable-static"
	"--disable-cxx"
	"--enable-assembly"
)

AB_FLAGS_O3=1
NOSTATIC=0
ABHOST=optenv32
